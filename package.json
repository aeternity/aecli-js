{
  "name": "@aeternity/aepp-cli",
  "version": "2.4.0",
  "description": "Aeternity command line interface",
  "bin": {
    "aecli": "./bin/aecli.js"
  },
  "dependencies": {
    "@aeternity/aepp-sdk": "^4.6.0",
    "child_process": "^1.0.2",
    "commander": "^2.19.0",
    "esm": "^3.0.84",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "prompts": "^2.0.3",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-ramda": "^1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "docco": "^0.8.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.0.0",
    "mocha-junit-reporter": "^1.17.0",
    "recursive-rename": "^2.0.0",
    "safe-buffer": "^5.1.2",
    "sinon": "^7.2.3",
    "standard": "*",
    "standard-loader": "^6.0.1",
    "supports-color": "^5.4.0"
  },
  "scripts": {
    "docs": "docco -t tooling/docs/markdown.jst -c /dev/null bin/*.js && docco -t tooling/docs/markdown.jst -c /dev/null bin/commands/*.js && docco -t tooling/docs/markdown.jst -c /dev/null bin/utils/*.js &&  rm -f docs/null && node rename-to-md.js",
    "test": "mocha --recursive --require @babel/register",
    "test:watch": "mocha --recursive --require @babel/register --watch",
    "test-jenkins": "mocha --recursive --require @babel/register --reporter mocha-junit-reporter && rm -rf node_modules/",
    "genChangelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublishOnly": "npm run docs"
  },
  "keywords": [
    "CLI",
    "aeternity",
    "blockchain"
  ],
  "author": "nduchak",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
